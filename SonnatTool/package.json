  "scripts": {
    "watch-sass": "node-sass src/assets/scss/bundle.scss src/assets/css/bundle.css --watch",
    "compile-sass": "node-sass src/assets/scss/bundle.scss src/assets/css/bundle.css",
    "prefix-css": "postcss src/assets/css/bundle.css --use autoprefixer -o src/assets/css/bundle.prefix.css",
    "compress-css": "node-sass src/assets/css/bundle.prefix.css src/assets/css/bundle.min.css --output-style compressed",
    "build-css": "npm-run-all compile-sass prefix-css compress-css"
  },
